<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.mapping.DoctorMapper">
    <select id="findDoctorByName" parameterType="java.lang.String" resultType="cn.com.entity.Doctor">
        select * from doctor where name like CONCAT('%',#{name},'%') limit 1,20
    </select>
    <select id="findDoctor" resultType="cn.com.entity.Doctor">
        select * from doctor limit 1,20
    </select>
</mapper>